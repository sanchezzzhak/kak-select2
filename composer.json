{
  "name": "kak/select2",
  "description": "select2 widgets for Yii2",
  "type": "library",
  "keywords": [
    "yii2",
    "select2 yii2",
    "kak select2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tutik Alexsandr",
      "email": "sanchezzzhak@ya.ru"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/select2": "^4.0",
    "bower-asset/jquery-slimscroll": "*"
  },
  "require-dev": {
    "symfony/event-dispatcher-contracts": ">=2.2.0",
    "codeception/codeception": ">=4.1.22",
    "codeception/module-asserts": ">=1.3",
    "codeception/module-yii2": ">=1.1"
  },
  "autoload": {
    "psr-4": {
      "kak\\widgets\\select2\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": false
    }
  }
}
